{
    "extends": "../tslint.json",
    "rules": {
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "no-input-rename": false,
        "one-line": [
            true,
            "check-catch",
            "check-whitespace"
        ],
        "no-inferrable-types": [
            false
        ],
        "no-output-rename": false
    }
}